    <div class="card feed-container">
      <div class="card-header ch-alt m-b-20">
        <h2>Feed<small>Community feed</small></h2>
      </div>
      <div class="card-body">
        <div class="listview">
        <% @feeds.each do |feed| %>
          <a class="lv-item">
            <div class="media">
              <div class="pull-left">
              <%= image_tag feed.user.avatar, class: "lv-img-sm" %>
              </div>
              <div class="media-body">
                <div class="lv-title"><%= feed.user.name %></div>
                <small class="lv-small"><%= render "feeds/#{feed.trackable_type.underscore}/#{feed.action}", feed: feed %></small>
              </div>
            </div>
          </a>
        <%end%>
        </div>
      </div>
    </div>
